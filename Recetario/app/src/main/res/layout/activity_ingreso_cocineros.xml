<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ingreso_cocineros">

    <EditText
        android:id="@+id/etFechaIntegracion"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="dd-mm-yyyy"
        android:inputType="text"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="232dp" />

    <RadioButton
        android:id="@+id/rbtAutorSi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Si"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="352dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="102dp"
        android:layout_height="39dp"
        android:text="Es el autor?"
        android:textColor="@color/black"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="309dp" />

    <RadioButton
        android:id="@+id/rbtAutorNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No"
        tools:layout_editor_absoluteX="219dp"
        tools:layout_editor_absoluteY="352dp" />

    <EditText
        android:id="@+id/etNombreC"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="text"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="103dp" />

    <EditText
        android:id="@+id/etEdad"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Edad"
        android:inputType="text"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="164dp" />

    <EditText
        android:id="@+id/etCostumerScore"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Score"
        android:inputType="text"
        tools:layout_editor_absoluteX="219dp"
        tools:layout_editor_absoluteY="164dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView3"
        android:layout_width="299dp"
        android:layout_height="188dp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="452dp" />

    <Button
        android:id="@+id/btnGuardarC"
        android:layout_width="103dp"
        android:layout_height="49dp"
        android:background="@color/yellow"
        android:text="Guardar"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="660dp" />

    <ImageButton
        android:id="@+id/imbBackCV"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toTopOf="@+id/etNombreC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.471"
        app:srcCompat="@drawable/baseline_arrow_back_24" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Elija sus recetas"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="398dp" />

</androidx.constraintlayout.widget.ConstraintLayout>